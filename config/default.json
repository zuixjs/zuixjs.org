{
  "zuix": {
    "build": {
      "input": "source",
      "output": "docs",
      "copy": [
        "css",
        "images",
        ".nojekyll",
        "browserconfig.xml",
        "manifest.json",
        "humans.txt",
        "favicon.ico",
        "robots.txt",
        "pages/**/*.jpg",
        "pages/**/*.png",
        "pages/**/*.webp",
        "pages/**/*.gif",
        "pages/**/*.svg",
        "pages/**/*.json"
      ],
      "ignore": [
        "_filters",
        "pages/**/_inc/*"
      ],
      "dataFolder": "_data",
      "includesFolder": "_inc",
      "componentsFolders": [ "app" ],
      "contentFolder": "pages",
      "dependencies": [{
        "zuix-dist/js": "js/zuix",
        "fuse.js/dist": "js/fuse",
        "flex-layout-attribute/css": "css/fla",
        "animate.css/animate.min.css": "css"
      }],
      "bundle": {
        "js": false,
        "css": false,
        "zuix": false
      },
      "mirror": "",
      "serviceWorker": false,
      "minify": {
        "disable": true,
        "collapseWhitespace": true,
        "removeOptionalTags": true,
        "removeRedundantAttributes": true,
        "removeScriptTypeAttributes": true,
        "removeTagWhitespace": false,
        "useShortDoctype": false,
        "collapseBooleanAttributes": true,
        "removeAttributeQuotes": false,
        "removeEmptyAttributes": true,
        "minifyCSS": false,
        "minifyJS": true
      }
    },
    "app": {
      "title": "zuix.js",
      "subtitle": "Presentation and documentation website.",
      "language": "en",
      "baseUrl": "/",
      "resourcePath": "/app/",
      "libraryPath": {
        "@lib": "https://zuixjs.github.io/zkit/lib/1.2/",
        "@hgui": "https://genielabs.github.io/homegenie-web-ui/app/",
        "@cdnjs": "https://cdnjs.cloudflare.com/ajax/libs/"
      },
      "zuixjs.github.io": {
        "baseUrl": "/zuixjs.org/",
        "resourcePath": "/zuixjs.org/app/"
      }
    }
  }
}
